<li class="card-item col-xs-12 {{size_class}} {{className}}" data-id="{{id}}" data-sort-position="{{position_group}}">
    <div class="wrapper">
        <div class="rank-column background-theme">
            <!-- <div class="tier">Tier 1</div> -->
            <div class="rank">{{rank}}</div>
            <div class="image">
                <img src="img/players/{{filter_id}}.png" alt="{{name}}" />
            </div>
            {{#if coverage.length}}
                <div class="ringer-coverage">
                    <h5>Ringer Coverage</h5>
                    <ul>
                        {{#each coverage}}
                            <li>
                                <a href="{{this.link}}" target="_blank">{{this.headline}}</a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
            {{/if}}
        </div>
        <div class="info-column">
            <div class="medium-show">
                <div class="player-main">
                    <h3>{{name}}</h3>
                    <h4><strong>{{position}}</strong></h4>
                    <h4>{{school}}</h4>
                </div>
                <div class="player-stats">
                    <ul>
                        {{#each stats}}
                            <li>
                                <div class="stat-wrap">
                                    <span class="title">{{this.title}}</span>
                                    <span class="number">{{this.number}}</span>
                                    <span class="detail color-theme">{{this.detail}}</span>
                                </div>
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="player-meta">
                    <ul>
                        <div class="medium-column">
                            <li class="ringer-stat color-theme"><span class="label non-mobile">RINGER&rsquo;S 1 REASON</span><span class="label is-mobile">1 REASON:</span> <span class="value">{{stat}}</span></li>
                            <li><span class="label">Age</span><span class="value">{{age}}</span></li>
                        </div>
                        <div class="medium-column">
                            <li><span class="label">Height</span><span class="value">{{height}}</span></li>
                            <li><span class="label">Wingspan</span><span class="value">{{wingspan}}</span></li>
                        </div>
                        <div class="medium-column">
                            <li><span class="label">Standing Reach</span><span class="value">{{reach}}</span></li>
                        </div>
                    </ul>
                </div>
                <div class="player-description">
                    <div class="player-headline">{{{summary}}}</div>
                    <div class="player-subline"><strong>ROLE MODELS: </strong>{{role_models}}</div>
                </div>
            </div>
            <div class="player-info">
                <div class="player-stat-image">
