<li class="card-item col-xs-12 small {{className}}" data-id="{{id}}">
    <div class="wrapper">
        <div class="rank-column background-theme">
            <div class="tier">Tier 1</div>
            <div class="rank">{{rank}}</div>
            <div class="image">
                <img src="img/players/{{filter_id}}.png" alt="{{name}}" />
            </div>
            {{#if coverage.length}}
                <div class="ringer-coverage">
                    <h5>Ringer Coverage</h5>
                    <ul>
                        {{#each coverage}}
                            <li>
                                <a href="{{this.link}}" target="_blank">{{this.headline}}</a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
            {{/if}}
        </div>
        <div class="info-column">
            <div class="player-main">
                <h3>{{name}}</h3>
                <h4><strong>{{position}}</strong></h4>
                <h4>{{school}}</h4>
            </div>
            <div class="player-stats">
                <ul>
                    {{#each stats}}
                        <li>
                            <div class="stat-wrap">
                                <span class="title">{{this.title}}</span>
                                <span class="number">{{this.number}}</span>
                                <span class="detail color-theme">{{this.detail}}</span>
                            </div>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="player-info">
                <div class="player-meta">
                    <ul>
                        <div class="medium-column">
                            <li class="ringer-stat color-theme"><span class="label">RINGERâ€™S 1 REASON</span><span class="value">{{ringer_stat}}</span></li>
                            <li><span class="label">Age</span><span class="value">{{age}}</span></li>
                        </div>
                        <div class="medium-column">
                            <li><span class="label">Height</span><span class="value">{{height}}</span></li>
                            <li><span class="label">Wingspan</span><span class="value">{{wingspan}}</span></li>
                        </div>
                        <div class="medium-column">
                            <li><span class="label">Standing Reach</span><span class="value">{{reach}}</span></li>
                        </div>
                    </ul>
                </div>
                <div class="player-description">
                    <div class="player-headline">{{{summary}}}</div>
                    <div class="player-subline"><strong>BEST CASE: </strong>{{best_case}}&nbsp;&nbsp;&nbsp;<strong>WORST CASE: </strong>{{worst_case}}</strong></div>
                </div>
                <div class="player-stat-image">
                    <div>
                        <div style="background-image:url(img/stat-image.jpg);" class="default"></div>
                        {{#each plus}}
                            {{#if this.media}}
                                <div style="background-image:url({{this.media}});" class="plus-minus-media" data-id="{{this.media}}" ></div>
                            {{/if}}
                        {{/each}}

                        {{#each minus}}
                            {{#if this.media}}
                                <div style="background-image:url({{this.media}});" class="plus-minus-media" data-id="{{this.media}}" ></div>
                            {{/if}}
                        {{/each}}
                    </div>
                </div>
                <div class="player-plus-minus">
                    <h6 class="color-theme">PLUSES</h6>
                    <ul>
                        {{#each plus}}
                            {{#if this.media}}
                                <li class="has-media" data-media="{{this.media}}"><div class="media-marker background-theme-after border-theme-before"></div>{{{this.text}}}</li>
                            {{else}}
                                <li>{{{this.text}}}</li>
                            {{/if}}
                        {{/each}}
                    </ul>
                    <h6 class="color-theme">MINUSES</h6>
                    <ul>
                        {{#each minus}}
                            {{#if this.media}}
                                <li class="has-media" data-media="{{this.media}}"><div class="media-marker background-theme-after border-theme-before"></div>{{{this.text}}}</li>
                            {{else}}
                                <li>{{{this.text}}}</li>
                            {{/if}}
                        {{/each}}
                    </ul>
                </div>
                <div class="player-social">
                    <a href="https://www.facebook.com/sharer/sharer.php?u=http%253A%252F%252Fmikemorisco.com%252Fringer%252Fnba%252F%3Fplayer%3D{{id}}" target="_blank" class="facebook">Share To Facebook</a>
                    <a href="https://twitter.com/intent/tweet?url=http://mikemorisco.com/ringer/nba/?player={{id}}&text={{twitter_share}}" target="_blank" class="twitter">Share To Twitter</a>
                </div>
            </div>
        </div>
        <div class="toggle-card" data-id="{{id}}">
            <?xml version="1.0" encoding="utf-8"?><svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 32 32" style="enable-background:new 0 0 32 32;" xml:space="preserve"><style type="text/css">.stroke{clip-path:url(#SVGID_2_);fill:none;stroke:#4CB96E;stroke-width:2;}.arrow{clip-path:url(#SVGID_4_);fill:#4CB96E;}</style><g><g><defs><rect id="SVGID_1_" x="1" y="1" width="30" height="30"/></defs><clipPath id="SVGID_2_"><use xlink:href="#SVGID_1_" style="overflow:visible;"/></clipPath><circle class="stroke" cx="16" cy="16" r="14"/></g><g><defs><rect id="SVGID_3_" x="1" y="1" width="30" height="30"/></defs><clipPath id="SVGID_4_"><use xlink:href="#SVGID_3_" style="overflow:visible;"/></clipPath><path class="arrow" d="M13.9,19.1c0,0,2,2.3,4,0l3.5-4c0,0,2-2.3-1-2.3h-9c0,0-3,0-1,2.3L13.9,19.1z"/></g></g></svg>
        </div>
    </div>
</li>
